{
  "extends": "tsconfig/nextjs.json",
  "compilerOptions": {
    "downlevelIteration": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./src/*"],
      "@/*": ["./src/modules/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "theme.config.js"],
  "exclude": ["node_modules"]
}
